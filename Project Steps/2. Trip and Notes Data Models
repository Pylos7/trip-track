Set Up Models
in models.py create models and model out out tables
class Trip(models.Model):
	city = models.CharField(max_length=50)
	country = models.CharField(max_length=2)
	start_date = models.DateField(blank=True, null=True)
	end_date = models.DateField(blank=True, null=True)
	for owner field import get_user_model from django.contrib.auth
	make User var by calling get_user_model
	owner = models.ForeignKey(User, on_delete=models.CASCADE, related_name='trips')
	overwrite dunder str method
class Note(models.Model):
	EXCURSIONS = (
		('event', 'Event'),
		('dining', 'Dining'),
		('experience', 'Experience'),
		('general', 'General'),
	)

	trip = models.ForeignKey(Trip, on_delete=models.CASCADE, related_name='notes')
	name = models.CharField(max_length=100)
	description = models.TextField()
	type = models.CharField(max_length=100, choices=EXCURSIONS)
	img = models.ImageField(upload_to='notes', blank=True, null=True)
	for rating validation 1 - 5 import MaxValueValidator from django.core.validators
	rating = models.PositiveSmallIntegerField(default=1, validators=)

	def __str__(self):
		return f"{self.name} in {self.trip.city}"