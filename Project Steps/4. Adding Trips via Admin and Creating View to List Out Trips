create superuser
py manage.py createsuperuser
run server and add use models
create view
	import models
		from .models import Trip, Note
	def trips_list(request):
		trips = Trip.objects.all()
		context = {
			'trips': trips
		}
		return render(request, 'trip/trip_list.html')
set up url
	import trips_list from .models
	path('dashboard/', trips_list, name='trip-list'),
create template for the above
templates/trip/trips_list.html
	<h1>My Trips</h1>
<ul>
	{% for trip in trips %}
	<li>{{ trip.country }} | {{ trip.city }}</li>
	{% endfor %}
</ul>
